{
  "name": "Blackjack-Adviser",
  "version": "1.0.0",
  "description": "A simple web application that suggests the best statistical blackjack betting stratagy",
  "main": "index.js",
  "repository": "git@github.com:JPStrydom/Blackjack-Adviser.git",
  "scripts": {
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "postinstall": "webpack -p",
    "start": "node server.js",
    "test": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec \"src/**/*.spec.js\"",
    "prettier": "prettier --write --tab-width=4 --single-quote=true --print-width=120 \"src/**/*.{js,css}\""
  },
  "pre-commit": [
    "prettier"
  ],
  "author": "JP Strydom",
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "istanbul": "0.4.5",
    "jest": "21.2.1",
    "mocha": "5.0.1",
    "pre-commit": "1.2.2",
    "prettier": "1.8.2",
    "webpack-dev-server": "2.9.3"
  },
  "dependencies": {
    "babel": "6.23.0",
    "babel-core": "6.2.1",
    "babel-jest": "21.0.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.1.18",
    "cardsJS": "1.1.1",
    "express": "4.6.1",
    "lodash": "4.17.4",
    "react": "16.0.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "16.0.0",
    "react-items-carousel": "2.2.1",
    "react-redux": "5.0.6",
    "react-simple-dropdown": "3.2.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "webpack": "3.8.1"
  }
}
